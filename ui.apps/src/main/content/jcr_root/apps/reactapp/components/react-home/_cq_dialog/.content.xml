<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="React - Home Page"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <layout jcr:primaryType="nt:unstructured"
              margin="{Boolean}false"
              sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
      <items jcr:primaryType="nt:unstructured">
         <column jcr:primaryType="nt:unstructured"
                 sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
                <phase1 cq-msm-lockable="./phase1"
                        fieldLabel="Digite a primeira frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase1"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <phase2 cq-msm-lockable="./phase2"
                        fieldLabel="Digite a segunda frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase2"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <phase3 cq-msm-lockable="./phase3"
                        fieldLabel="Digite a terceira frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase3"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <phase4 cq-msm-lockable="./phase4"
                        fieldLabel="Digite a quarta frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase4"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <phase5 cq-msm-lockable="./phase5"
                        fieldLabel="Digite a quinta frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase5"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <phase6 cq-msm-lockable="./phase6"
                        fieldLabel="Digite a sexta frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase6"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <phase7 cq-msm-lockable="./phase7"
                        fieldLabel="Digite a sétima frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase7"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <phase8 cq-msm-lockable="./phase8"
                        fieldLabel="Digite a oitava frase"
                        jcr:primaryType="nt:unstructured"
                        name="./phase8"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <warning cq-msm-lockable="./warning"
                        fieldLabel="Digite um texto de aviso"
                        jcr:primaryType="nt:unstructured"
                        name="./warning"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <continua cq-msm-lockable="./continua"
                        fieldLabel="Digite o nome do botão de continuar navegando"
                        jcr:primaryType="nt:unstructured"
                        name="./continua"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <logout cq-msm-lockable="./logout"
                        fieldLabel="Digite o nome do botão de logout"
                        jcr:primaryType="nt:unstructured"
                        name="./logout"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
            </items>
        </column>
      </items>
   </content>
</jcr:root>
